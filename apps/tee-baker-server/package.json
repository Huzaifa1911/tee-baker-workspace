{
  "name": "tee-baker-server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "db-up": "dotenv -e .env.development -- docker compose up dev-db -d",
    "db-down": "dotenv -e .env.development -- docker compose rm dev-db -s -f -v",
    "migrate": "dotenv -e .env.development npx prisma migrate deploy",
    "create-migrate": "dotenv -e .env.development npx prisma migrate dev",
    "db-seed": "dotenv -e .env.development npx prisma db seed",
    "db-restart": "nx run tee-baker-server:db-down && nx run tee-baker-server:db-up && sleep 1 && nx run tee-baker-server:migrate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/nest": "19.7.2",
    "@nx/node": "19.7.2",
    "@nx/webpack": "19.7.2",
    "jest-environment-node": "^29.7.0",
    "prisma": "^5.19.1",
    "webpack-cli": "^5.1.4"
  }
}
