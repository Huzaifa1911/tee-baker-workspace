{
  "singleQuote": true,
  "printWidth": 150,
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "trailingComma": "all",

   "importOrder": ["^@modules/(.*)$", "^@common/(.*)$", "^@shared/(.*)$","^@/assets(.*)$", "^@/components(.*)$", "^@/icons(.*)$", "^@/navigation(.*)$", "^@/screens(.*)$", "^@/services(.*)$", "^@/styles(.*)$", "^@/types(.*)$", "^@/utils(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
